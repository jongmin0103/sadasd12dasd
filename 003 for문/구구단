import java.util.*;

public class Main
{
    public static void main(String[] args)
    {
        // 입력하기
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        sc.close();
        
        // 출력하기
        for(int i = 1; i < 10; i++)
            System.out.println(a + " * " + i + " = " + a * i );
        
    }    
}
