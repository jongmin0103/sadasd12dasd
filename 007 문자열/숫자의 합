import java.util.*;

public class Main
{
	public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        
        int answer = 0;
        int num = sc.nextInt();
        String sut = sc.next();
        long sum = 0;
        
        sc.close();
            
        for(int i = 0; i < num; i++)
        {
            sum += sut.charAt(i) - '0';
        }
        
        System.out.println(sum);
        
    }
    
}
