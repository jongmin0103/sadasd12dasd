import java.util.*;

public class Main
{
	public static void main(String[] args)
    {
        // 입력하기
        Scanner sc = new Scanner(System.in);
        
        int num = sc.nextInt();
        int[] answer = new int[num];
        
        for(int i = 0; i < num; i++)
        {
            answer[i] = sc.nextInt();
        }
        
        sc.close();
        
        // 출력하기
        Arrays.sort(answer);
        
        System.out.println(answer[0] + " " + answer[num - 1]);
    }
    
}
